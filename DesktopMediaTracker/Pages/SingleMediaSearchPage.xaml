<Page x:Class="DesktopMediaTracker.Pages.SingleMediaSearchPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DesktopMediaTracker.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="304" d:DesignWidth="520"
      Title="SingleMediaSearchPage"
      >

    <Grid Height="280" VerticalAlignment="Top">

        <Rectangle Fill="LavenderBlush"  HorizontalAlignment="Left" Height="80" Stroke="Black" VerticalAlignment="Top" Width="520"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="500"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="403,49,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="97">
                <Hyperlink NavigateUri="AdvancedSearchPage.xaml?Message=Hello">Advanced Search</Hyperlink>
        </TextBlock>
        <ScrollViewer HorizontalAlignment="Left" Height="195" Margin="0,85,0,0" VerticalAlignment="Top" Width="520">
            <StackPanel Height="180" Width="507"/>
        </ScrollViewer>
        <Label x:Name="uxFirst" Content="1, " HorizontalAlignment="Left" Margin="196,280,0,-29" VerticalAlignment="Top" Height="29" Width="24" TextBlock.Foreground="Blue" TextBlock.TextAlignment="Right" RenderTransformOrigin="0.417,-2.241"/>
        <Label x:Name="uxNext" Content="2,  " HorizontalAlignment="Left" Margin="220,280,0,-29" VerticalAlignment="Top" Height="29" Width="22" TextBlock.Foreground="Blue"/>
        <Label x:Name="uxNextTwo" Content="3,  " HorizontalAlignment="Left" Margin="242,280,0,-29" VerticalAlignment="Top" Height="29" Width="22" TextBlock.Foreground="Blue"/>
        <Label x:Name="uxLast" Content="Last" HorizontalAlignment="Left" Margin="269,280,0,-29" VerticalAlignment="Top" Height="29" Width="37" TextBlock.Foreground="Blue" RenderTransformOrigin="0.73,0.345"/>
        
            
        

    </Grid>
</Page>
